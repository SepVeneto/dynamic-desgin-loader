{
	"name": "dynamic-vscode",
  "publisher": "sepveneto",
	"displayName": "dynamic-vscode",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:dynamic-vscode.open"
	],
	"main": "./extension.js",
	"contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "dynamic-vscode.url": {
          "type": "string",
          "default": "https://sepveneto.github.io/dynamic-form-making-vue3/",
          "description": "设计器地址"
        }
      }
    },
		"commands": [
			{
				"command": "dynamic-vscode.open",
				"title": "打开设计器"
			}
		],
    "menus": {
      "explorer/context": [{
        "command": "dynamic-vscode.open",
        "group": "navigation"
      }]
    }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
